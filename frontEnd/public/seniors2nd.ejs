<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Meet Your Seniors</title>
  <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;600&family=Raleway:wght@400;600&display=swap"
    rel="stylesheet">

  <link rel="stylesheet" href="/styles/navbar.css" />
  <link rel="stylesheet" href="/styles/theme.css" />
  <link rel="stylesheet" href="/styles/demo.css">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/vue/2.0.1/vue.min.js"></script>
</head>

<body>
  <%- include("./partials/nav-up.ejs") %>
  <div class="page-content">
    <div class="container">
      <div class="nav-wrapper">
        <div class="left-side">
          <div class="gallery-title">
            <h1 class="main_heading">Meet Your Seniors</h1>
          </div>
        </div>
      </div>
      <!-- 440 x 320 -->
      <!-- #1  -->
      <div id="app" class="wrpr">
        
        <% for(var i=0;i<len;i++){%>
          <card data-image="<%= data.responses[i].Profile%>">
            <h1 class="StuName" slot="header"><%= data.responses[i].Name%></h1>
            <p slot="content"><%= data.responses[i].Year%></p>
          </card>
        <%}%>

      </div>
    </div>
  </div>

  <%- include("./partials/nav-down.ejs") %>


  <script src="/scripts/seniors.js"></script>
</body>

</html>